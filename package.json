{
  "name": "react-menubar-starter",
  "version": "0.1.0",
  "description": "React menubar starter",
  "main": "main.js",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm:watch-*\"",
    "watch-el": "electron .",
    "watch-ui": "webpack --mode=development --watch",
    "build": "npm run build-ui && npm run packager",
    "build-ui": "webpack --mode=production",
    "packager": "electron-packager . HelloApp --platform=darwin --arch=x64 --icon=Icon.icns"
  },
  "dependencies": {
    "electron-packager": "15.3.0",
    "menubar": "^9.0.5",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "concurrently": "6.2.1",
    "swc-loader": "0.1.14",
    "@swc/cli": "0.1.49",
    "@swc/core": "1.2.82",
    "electron": "13.2.2",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0"
  }
}
